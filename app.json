{
  "expo": {
    "name": "Calm Elevation",
    "slug": "calm-elevation",
    "icon": "./assets/cal-cloud.png",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/cal-cloud.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calmelevation.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cal-cloud.png",
        "backgroundColor": "#ffffff"
      },
      "foregroundService": {
        "enabled": false
      },
      "package": "com.calmelevation.app",
      "versionCode": 14,
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "favicon": "./assets/cal-cloud.png",
      "bundler": "metro"
    },
    "plugins": [
      "./plugins/removeBootReceiver",
      "expo-router",
      "expo-web-browser",
      "expo-font",
      "expo-notifications",
      "expo-asset"
    ],
    "scheme": "calm-elevation",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bc057bbb-acc3-4d04-a81d-be0206a62156"
      }
    }
  }
}
